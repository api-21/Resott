<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.taknikinigga.resotts">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:name=".Di.BaseApp"
    android:theme="@style/Theme.Risotto">
    <activity
        android:name="com.taknikinigga.resotts.Ui.Activities.SplashScreen"
        android:theme="@style/Theme.SplashTheme"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

    </activity>
    <activity android:name="com.taknikinigga.resotts.Ui.Activities.Authentication"> </activity>
    <activity android:name="com.taknikinigga.resotts.Ui.Activities.HomeActivity">
        <nav-graph android:value="@navigation/nav_home"></nav-graph>
    </activity>

</application>

</manifest>